  {
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "main.py"
      }
    ],

    "functions": {
      "main.py": {
        "runtime": "python3.9",
        "memory": 128,
        "maxDuration": 10
      }
    }
  }
  